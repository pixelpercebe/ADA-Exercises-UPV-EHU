WITH Ada.Integer_Text_IO, Ada.Text_IO;
USE Ada.Integer_Text_IO, Ada.Text_IO;

PACKAGE BODY Personas IS

   PROCEDURE Escribir_Personas (
         V : T_Vector_Personas) IS
   BEGIN
      FOR I IN V'RANGE LOOP
         Put(I, 3);
         Put(": ");
         Put(V(I).NombreApellidos);
         Put(" - ");
         Put(V(I).Ciudad);
         Put("- ");
         Put(Boolean'Image(V(I).Trabajando));
         Put(V(I).Edad, 3);
         New_Line;
      END LOOP;

   END Escribir_Personas;


   PROCEDURE Actualizar_Ciudad(V: IN OUT T_Vector_Personas; C1, C2: String) IS

   BEGIN
      FOR I IN V'RANGE LOOP
         IF V(I).Ciudad = C1 THEN
            c2 := c2

      end loop;

   end actualizar_ciudad;

END Personas;